{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "algolia/algoliasearch-client-php": "^2.3",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.8.*",
        "league/commonmark": "^0.18.3",
        "sentry/sentry-laravel": "2.7.0",
        "spatie/laravel-collection-macros": "^6.0",
        "spatie/laravel-feed": "2.4.*",
        "spatie/laravel-responsecache": "^4.0",
        "spatie/laravel-robots-middleware": "^1.0",
        "spatie/laravel-sitemap": "^5.3",
        "spatie/yaml-front-matter": "^2.0.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
